@model Person

@{
    ViewBag.Title = "Create Person";
}

<h1>@ViewBag.Title</h1>

<form method="post">
    <p>Are you sure you want to delete @Model.FirstName @Model.LastName?</p>
    <a asp-action="Index" class="btn btn-light">No</a>
    <button type="submit" class="btn btn-danger">Yes</button>
</form>